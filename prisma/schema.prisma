datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["selectRelationCount", "orderByRelation", "orderByAggregateGroup"]
}

model User {
    id String @id @default(dbgenerated("nanoid()")) @db.VarChar(11)
    discordId String @unique

    username String
    access String? @unique
    refresh String? @unique
}
